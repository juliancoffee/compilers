func main() {
    let a: Int = 2;
    let b: Int = 3;
    let c: Int = 2; // для 2**3**2

    // Перевірка: 2**3**2 = 512.0
    let base: Double = a ** b ** c;

    // base * 8.0 = 4096.0. negResult = -4096.0
    var negResult: Double = -(base * 8.0);

    // 10.0 - 20.0 - (-4096.0) = -10.0 + 4096.0 = 4086.0
    let check: Double = 10.0 - 20.0 - negResult;

    print("");
    print("Результат: ", check);
    print("Число: ", negResult, "");
}
