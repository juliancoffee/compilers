let maxCount: Int = 5;

func factorial(n: Int) -> Int {
    if n <= 1 {
        return 1;
    }
    if n > maxCount {
        return 0;
    }
    let res = n * factorial(n - 1);
    print("Factorial of", n, "=", res);
    return res;
}

func main() {
    let num: Int = 5;
    let result = factorial(num);
    print("Факторіал", num, ":", result);
}
