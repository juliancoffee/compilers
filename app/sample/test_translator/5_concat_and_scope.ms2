let baseValue = 10; // Глобальна змінна

func addAndFormat(val: Double) -> Double {
    print("BaseValue =", baseValue);
    let b = 5.0;
    let sum = val + b + baseValue;
    print("Сума для b " + "=", b, "і val =", val, "становить", sum);
    return sum;
}
func calculate(a: Int) -> Double {
    let b = 5.0;
    let finalSum = a + b;

    let formattedSum = addAndFormat(finalSum);
    return finalSum;
}
func main() {
    let result = calculate(baseValue);
    print("Результат:", result);
}
