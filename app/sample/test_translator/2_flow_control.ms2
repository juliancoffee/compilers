func main() {
    var counter = 0;
    let limit = 10;

    let remainder = 11 / 2 - 5;
    print("Залишок: ", remainder);   // 0.5

    print("Парні числа: ");
    while (counter <= limit) {
        if (counter == 0.0) {
            print("Початок циклу");
        } else {
            print(counter);
        }
        counter = counter + 2;
    }
}
