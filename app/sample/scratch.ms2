func main() {
    let choice = 4;
    print("Comparing choice:", choice, "by pattern");
    switch choice {
        case 1 {
            print("Choice is 1");
        }
        case range(0, 10) {
            print("Choice is 0 to 10");
        }
        case range(0, 5) {
            print("Choice is 0 to 5");
        }
    }
}
